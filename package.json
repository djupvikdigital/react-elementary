    "enzyme": "^2.3.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^5.0.0",
    "eslint-plugin-import": "^2.2.0",
    "expect": "^1.20.1",
    "immutable": "^3.8.1",
    "jsdoc": "^3.4.0",
