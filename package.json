    "@types/ramda": "^0.24.14",
    "babel-core": "^6.26.0",
    "babel-plugin-ramda": "^1.4.3",
    "babel-preset-env": "^1.6.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "expect": "^21.2.1",
    "gulp": "^3.9.1",
