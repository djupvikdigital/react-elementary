    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.7.2",
    "expect": "^21.1.0",
    "immutable": "^3.8.1",
    "jsdoc": "^3.5.5",
    "mocha": "^3.5.3",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0"
  },
  "dependencies": {
